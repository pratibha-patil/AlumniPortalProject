<h1>Student Names</h1>
<table>
		<tr>
			<th>Name</th>
			<th>Project</th>
			
		</tr>
		

		<% @projectaccepted.each do |prjct|%>
		<tr><td><%= prjct.project.student.first_name %>&nbsp;<%= prjct.project.student.last_name%></td>
				 <td><%=link_to 'project' ,project_path(prjct.project) %></td>
				<td><%= prjct.project.project_name %></td>
				<td><%= link_to "#{prjct.project.student.first_name} #{prjct.project.student.last_name}",student_path(prjct.project.student)%></td>
			</tr>
		<% end %>
</table>