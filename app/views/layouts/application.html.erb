<!DOCTYPE html>
<html>
<head>
  <title>AlumniPortal</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_student%> 
  <ul>
    
    <li><%= current_student.email %></li> 
    <li><%= link_to "Log out", logout_path, method: "delete" %></li> 
    
  </ul> 
<% else %> 
  <ul> 
    <% if !current_alumni %> 
    <li><%= link_to "LoginasStudent", 'login' %></a></li> 
    <li><%= link_to "SignupasStudent", 'signup' %></a></li> 
    <% end %>
  </ul> 
<% end %>

<% if current_alumni%> 
  <ul> 
    <li><%= current_alumni.email %></li> 
    <li><%= link_to "Log out", logoutalumni_path, method: "delete" %></li> 
  </ul> 
<% else %> 
  <ul> 
    <% if !current_student %>
    <li><%= link_to "LoginasAlumni", 'loginalumni' %></a></li> 
    <li><%= link_to "SignupasAlumni", 'signupalumni' %></a></li> 
    <% end %>
  </ul> 
<% end %>

<%= yield %>

</body>
</html>
